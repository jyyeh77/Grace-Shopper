app.factory('AdminFactory', function ($http) {
  let AdminFactory = {};
  
  AdminFactory.changeAdminStatus = function (userEmail) {
    console.log(`Making ${userEmail} an admin..`)
    return $http({
        method: 'GET',
        url: `api/users?email=${userEmail}`,
    })
    .then(foundUser => {
        console.log('foundUser is', foundUser)
        // $http({
        //     method:'PUT',
        //     url: `api/users/`
        // })
    })
  }

  return AdminFactory;
});
